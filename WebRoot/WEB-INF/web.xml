<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>GetPictureUrls</servlet-name>
    <servlet-class>picture.GetPictureUrls</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SavePicture</servlet-name>
    <servlet-class>picture.SavePicture</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetPictureUrls</servlet-name>
    <url-pattern>/GetPictureUrls</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SavePicture</servlet-name>
    <url-pattern>/SavePicture</url-pattern>
  </servlet-mapping>
</web-app>